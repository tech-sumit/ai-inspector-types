{
  "name": "@anthropic/ai-inspector-types",
  "version": "0.1.0",
  "description": "Shared TypeScript types and protocol schemas for the AI Inspector ecosystem",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "keywords": ["webmcp", "ai-inspector", "types", "chrome-devtools-protocol"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tech-sumit/ai-inspector-types"
  }
}
